<div>
    <div class="subalbums">
        {% for album in subAlbums %}
            <div class="subalbum">
                <a href="{{ album.slug }}">
                    <img src="/img/folder.png" width="100" height="100" title="Album: {{ album.title }}">
                    <div class="caption">
                        {{ album.title }}
                    </div>
                </a>
            </div>
        {% endfor %}
    </div>
</div>

<div class="clearfix"></div>

